pipeline {
    agent any
    stages {
        stage('Clean') {
            steps {
                echo 'Cleaning...'
                bat 'mvn clean'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                bat 'mvn test'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging...'
                bat 'mvn package'
            }
        }
        stage('Build') {
            steps {
                echo 'Building...'
                bat 'mvn install'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                bat 'mvn deploy'
            }
        }
    }
}
